  "devDependencies": [
    "eslint",
    "prettier",
    "eslint-config-prettier",
    "eslint-plugin-prettier",
    "@rollup/rollup-win32-x64-msvc"
  ],
  "directories": {
    "src/": {
      "path": "./src",
      "purpose": "Application source code and content templates",
      "files": {
        "main.js": "Main application entry point"
      }
    },
    "scripts/": {
      "path": "./scripts",
      "purpose": "Automation and deployment scripts"
    },
    "data/": {
      "path": "./data",
      "purpose": "Data storage with standardized structure",
